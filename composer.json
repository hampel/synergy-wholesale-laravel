{
    "name": "hampel/synergy-wholesale-laravel",
	"homepage": "https://bitbucket.org/hampel/synergy-wholesale-laravel",
    "description": "Synergy Wholesale API wrapper using SoapClient and packaged as a Laravel service provider",
	"authors": [{
		"name": "Simon Hampel",
		"email": "simon@hampelgroup.com",
		"role": "Developer"
	}],
	"support": {
		"issues": "https://bitbucket.org/hampel/synergy-wholesale-laravel/issues",
		"source": "https://bitbucket.org/hampel/synergy-wholesale-laravel/src"
	},
	"keywords": [
		"Synergy Wholesale",
		"domains",
		"DNS",
		"API",
		"SOAP",
		"Laravel"
	],
	"license": [
		"MIT"
	],
    "require": {
        "illuminate/support": "^8.0|^9.0|^10.0",
		"hampel/synergy-wholesale": "^1.0",
		"php": ">=7.3"
    },
    "autoload": {
  		"psr-4" : {
			"SynergyWholesale\\" : "src/"
		}
    },
    "extra": {
		"laravel": {
			"providers": [
				"SynergyWholesale\\SynergyWholesaleServiceProvider"
			],
			"aliases": {
				"SynergyWholesale": "SynergyWholesale\\Facades\\SynergyWholesale"
			}
		}
    },
 	"minimum-stability": "dev",
	"prefer-stable": true
}
